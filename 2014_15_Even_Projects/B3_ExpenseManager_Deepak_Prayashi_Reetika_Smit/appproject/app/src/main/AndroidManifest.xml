<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smitd.appproject" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".projectmain"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity2"
            android:label="@string/title_activity_main_activity2"
            android:parentActivityName=".projectmain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.smitd.appproject.projectmain" />
        </activity>
        <activity
            android:name=".third_page"
            android:label="@string/title_activity_third_page" >
        </activity>
    </application>

</manifest>
